{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "zoneName": {
            "value": "arm.example"
        },
        "cnames": {
            "value": [
                {
                    "name": "test1",
                    "alias": "test1.azurewebsites.net",
                    "ttl": "3600"
                },
                {
                    "name": "test2",
                    "alias": "test2.azurewebsites.net",
                    "ttl": "3600"
                }
            ]
        },
        "aRecords": {
            "value": [
                {
                    "name": "www",
                    "ttl": 3600,
                    "records": [
                        {
                            "ipv4Address": "23.100.50.55"
                        },
                        {
                            "ipv4Address": "23.100.50.56"
                        }
                    ]
                }
            ]
        },
        "mxRecords": {
            "value": [
                {
                    "ttl": 3600,
                    "records": [
                        {
                            "exchange": "eu-smtp-inbound-1.mimecast.com",
                            "preference": 5
                        },
                        {
                            "exchange": "eu-smtp-inbound-2.mimecast.com",
                            "preference": 10
                        }
                    ]
                }
            ]
        },
        "txtRecords": {
            "value": [
                {
                    "name": "@",
                    "ttl": 3600,
                    "records": [
                        {
                            "value": [
                                "v=spf1 include:spf.protection.outlook.com -all"
                            ]
                        },
                        {
                            "value": [
                                "second TXT record value syntax for same root record"
                            ]
                        }
                    ]
                }
            ]
        },
        "srvRecords": {
            "value": [
                {
                    "ttl": 3600,
                    "name": "_sipfederationtls._tcp",
                    "port": 5061,
                    "priority": 100,
                    "target": "sipfed.online.lync.com",
                    "weight": 1
                },
                {
                    "ttl": 3600,
                    "name": "_sip._tls",
                    "port": 443,
                    "priority": 100,
                    "target": "sipdir.online.lync.com",
                    "weight": 1
                }
            ]
        },
        "vNetLinks": {
            "value": [
                {
                    "vNetNameToLink": "test-vnet"
                },
                {
                    "vNetNameToLink": "test-vnet2"
                }
            ]
        },
        "environment": {
            "value": "Global"
        },
        "buildver": {
            "value": "1.0.0"
        }
    }
}